<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">

        <title>Feed | Climbers Connect</title>

        <!-- Favicon -->
        <link rel="icon" href="/assets/mountain.png">
    
        <!-- Stylesheets -->
        <link rel="stylesheet" href="/css/reset.css">
        <link rel="stylesheet" href="/css/partials/header.css">
        <link rel="stylesheet" href="/css/partials/footer.css">
        
        <!-- Fonts -->
        <link rel="preconnect" href="https://fonts.googleapis.com">
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
        <link href="https://fonts.googleapis.com/css2?family=Raleway:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;1,100&display=swap" rel="stylesheet"> 

        <!-- Scripts -->
    </head>
    <body>
        <%- include('partials/header') -%>

        <h1>Feed</h1>

        <ul>
            <% for (let connect of connects) { %>
                <li>
                    <%= connect %>
                </li>
            <% } %>
        </ul>

        <ul>
            <% for (let climb of climbs) { %>
                <li>
                    <%= climb %>
                </li>
            <% } %>
        </ul>

        <%- include('partials/footer') -%>
    </body>
</html>
